{
  "name": "@ks89/ngx-drag-n-drop",
  "version": "1.0.0",
  "description": "Drag-n-Drop library for Angular >=7",
  "license": "MIT",
  "author": "Stefano Cappa",
  "homepage": "https://github.com/Ks89",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ks89/ngx-drag-n-drop.git"
  },
  "engines": {
    "node": ">=10.11.3",
    "npm": ">=6.4.1"
  },
  "scripts": {
    "ng": "ng",
    "start": "npm run serve",
    "serve": "npm run serve:dev",
    "serve:dev": "ng serve --aot",
    "serve:prod": "ng serve --prod  --aot --build-optimizer=true",
    "build": "npm run build:lib && npm run build:main",
    "build:lib": "ng build @ks89/ngx-drag-n-drop && npm run copy:build && npm run copy:other",
    "build:lib:watch": "ng build @ks89/ngx-drag-n-drop --watch",
    "build:main": "npm run build:main:dev",
    "build:main:dev": "ng build --aot",
    "build:main:prod": "ng build --prod --aot --build-optimizer=true",
    "build:all": "npm run build:lib && npm run build:main",
    "clean": "npm run clean:main",
    "clean:main": "npm run clean:dist && npm run clean:coverage && npm run clean:docs",
    "clean:dist": "rimraf @ks89",
    "clean:coverage": "rimraf coverage",
    "clean:docs": "rimraf docs",
    "clean:all": "npm run clean:main",
    "copy:build": "cpr dist/ks89/ngx-drag-n-drop @ks89/ngx-drag-n-drop -o",
    "copy:other": "cpr README.md @ks89/ngx-drag-n-drop/README.md -o && cpr LICENSE @ks89/ngx-drag-n-drop/LICENSE -o && cpr CHANGELOG.md @ks89/ngx-drag-n-drop/CHANGELOG.md -o && cpr CONTRIBUTING.md @ks89/ngx-drag-n-drop/CONTRIBUTING.md -o",
    "pretest": "npm run clean:coverage && npm run lint",
    "test": "ng test @ks89/ngx-drag-n-drop --watch=false --code-coverage=true",
    "test:snyk": "snyk test && npm run test",
    "test:watch": "ng test @ks89/ngx-drag-n-drop --watch=true --code-coverage=true",
    "test:ci": "ng test @ks89/ngx-drag-n-drop --watch=false --no-progress --code-coverage=true",
    "e2e": "ng e2e",
    "e2e:ee": "npm run e2e -- --element-explorer=true",
    "e2e:ci": "npm run e2e -- --protractor-config=e2e/protractor-ci.conf.js",
    "lint": "ng lint",
    "docs": "npm run docs:typedoc && npm run docs:compodoc",
    "docs:typedoc": "typedoc --out ./docs/typedoc --mode file --target ES6 --exclude **/*.spec.ts ./projects/ks89/ngx-drag-n-drop",
    "docs:typedoc:serve": "npm run docs:typedoc && lite-server --baseDir=\"docs/typedoc\"",
    "docs:compodoc": "compodoc projects/ks89/ngx-drag-n-drop -p tsconfig.json -d docs/compodoc",
    "docs:compodoc:serve": "npm run docs:compodoc -- -s",
    "docs:compodoc:serve:watch": "npm run docs:compodoc -- -s -w",
    "check:licenses": "license-checker --exclude 'MIT, MIT OR X11, BSD, ISC, Apache-2.0'",
    "coveralls": "coveralls < coverage/lcov.info"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --verbose"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.1",
    "@angular/common": "~8.0.1",
    "@angular/compiler": "~8.0.1",
    "@angular/core": "~8.0.1",
    "@angular/forms": "~8.0.1",
    "@angular/platform-browser": "~8.0.1",
    "@angular/platform-browser-dynamic": "~8.0.1",
    "@angular/router": "~8.0.1",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular-devkit/build-ng-packagr": "~0.800.0",
    "@angular/cli": "~8.0.4",
    "@angular/compiler-cli": "~8.0.1",
    "@angular/language-service": "~8.0.1",
    "@compodoc/compodoc": "^1.1.9",
    "@types/jasmine": "^3.3.13",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^12.0.3",
    "codelyzer": "^5.0.1",
    "coveralls": "^3.0.3",
    "cpr": "^3.0.1",
    "cross-env": "^5.2.0",
    "husky": "^2.3.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lite-server": "^2.5.3",
    "ng-packagr": "^5.2.0",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.11.0",
    "protractor": "^5.4.2",
    "rimraf": "^2.6.3",
    "sass-lint": "^1.13.1",
    "snyk": "^1.169.2",
    "sonar-scanner": "^3.0.3",
    "tsickle": "^0.35.0",
    "tslint": "^5.16.0",
    "typedoc": "^0.14.2",
    "typescript": "~3.4.3"
  }
}
